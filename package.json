{
    "name": "version-check-prompt",
    "version": "1.0.2",
    "description": "前端资源更新检查提示",
    "keywords": [
        "version",
        "chunk",
        "update",
        "upgrade",
        "check",
        "polling",
        "prompt"
    ],
    "homepage": "https://github.com/amoursun/version-check-prompt",
    "bugs": {
        "url": "https://github.com/amoursun/version-check-prompt/issues"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:amoursun/version-check-prompt.git"
    },
    "author": "gyl<gylssm@126.com>",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "browser": "./dist/umd/index.min.js",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "browser": "dist/umd/index.js",
    "unpkg": "dist/umd/index.min.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm run clean && rslib build",
        "dev": "rslib build --watch",
        "test": "vitest",
        "clean": "rimraf dist",
        "changeset": "changeset",
        "build:package": "zx script/package-config.js",
        "build:test:version": "zx scripts/generate-version.js -o dest/json",
        "prepare": "pnpm run build"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@changesets/cli": "^2.28.1",
        "@rslib/core": "^0.6.2",
        "@testing-library/jest-dom": "^6.6.3",
        "@types/node": "^22.14.1",
        "@types/shelljs": "^0.8.15",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "rsbuild-plugin-dts": "^0.6.3",
        "shelljs": "^0.9.2",
        "typescript": "^5.8.3",
        "vitest": "^3.1.1",
        "zx": "^8.5.2"
    },
    "packageManager": "pnpm@9.0.0",
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=9.0.0"
    }
}